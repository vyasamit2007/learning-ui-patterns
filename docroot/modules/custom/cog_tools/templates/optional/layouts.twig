# Cog Layouts
# - css generated by sass/layout/_panels.scss
{{ machine_name }}_one_col:
  label: One column
  category: {{ machine_name }}
  template: layouts/one_col
  default_region: main
  icon_map:
    - [main]
  regions:
    main:
      label: Main content
{{ machine_name }}_one_two_one:
  label: One two one
  category: {{ machine_name }}
  template: layouts/one_two_one
  default_region: middle_left
  icon_map:
    - [top]
    - [middle_left, middle_right]
    - [bottom]
  regions:
    top:
      label: Top content
    middle_left:
      label: Middle left content
    middle_right:
      label: Middle right content
    bottom:
      label: Bottom content
